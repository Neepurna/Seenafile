{
  "expo": {
    "name": "Inclivo",
    "slug": "inclivo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1",
      "bundleIdentifier": "com.cinephile.app",
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "versionCode": 1,
      "package": "com.inclivo.app",
      "softwareKeyboardLayoutMode": "pan",
      "screenOrientation": "portrait",
      "navigationBar": {
        "visible": "immersive"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "compileSdkVersion": 33,
      "targetSdkVersion": 33,
      "minSdkVersion": 21,
      "buildToolsVersion": "33.0.0",
      "gradle": {
        "kotlinVersion": "1.7.10"
      },
      "buildType": "apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you share them with your profile.",
          "cameraPermission": "The app needs access to your camera to let you take profile pictures."
        }
      ],
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "30f76484-486a-429e-bb06-a07fb346feb5"
      }
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/30f76484-486a-429e-bb06-a07fb346feb5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "jsEngine": "hermes",
    "packagerOpts": {
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "json"
      ]
    },
    "developmentClient": true,
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}

{
  "expo": {
    "name": "Inclivo",
    "slug": "inclivo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1",
      "bundleIdentifier": "com.cinephile.app",
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 1,
      "package": "com.cinephile.app",
      "softwareKeyboardLayoutMode": "pan",
      "screenOrientation": "portrait",
      "navigationBar": {
        "visible": "immersive"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-screen-orientation", {
        "initialOrientation": "PORTRAIT"
      }],
      ["expo-image-picker", {
        "photosPermission": "The app needs access to your photos to let you share them with your profile.",
        "cameraPermission": "The app needs access to your camera to let you take profile pictures."
      }]
    ],
    "extra": {
      "eas": {
        "projectId": "30f76484-486a-429e-bb06-a07fb346feb5"
      }
    },
    "owner": "neepurna",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "jsEngine": "hermes"
  }
}
